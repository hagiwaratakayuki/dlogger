    "@storybook/svelte": "^7.0.20",
    "@storybook/sveltekit": "^7.0.20",
    "@storybook/testing-library": "^0.1.0",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.5.0",
    "autoprefixer": "^10.4.14",
